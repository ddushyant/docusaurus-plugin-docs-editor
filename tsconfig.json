{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM"],
    "noEmit": true,
    "noImplicitAny": false,
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/plugin-content-docs"
    ],
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"],
      "@theme/*": ["./src/theme/*"]
    }
  }
}
